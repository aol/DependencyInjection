<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
">
    
    <bean id="messageService" class="dependencyinjection.example4.MessageService">
        <property name="input" ref="messageInput"/>
        <property name="output" ref="messageOutput"/>
    </bean>
    
    <bean id="messageInput" class="dependencyinjection.example4.RandomMessageProvider"/>
    
    <bean id="messageOutput" class="dependencyinjection.example4.ConsoleMessageOutput"/>


</beans>
